package date;

public class Date {
	public static String dateByWords(int a, int b, int c){
		String wordsdata = "";
		String day = "";
		return wordsdata;
		switch (a) 
		{
			case 1: day = "First"; break;
			case 2: day = "Second"; break;
			case 3: day = "Third"; break;
			case 4: day = "4st"; break;
			case 5: day = "5st"; break;
			case 6: day = "6st"; break;
			case 7: day = "7st"; break;

		}
		
	}

}
