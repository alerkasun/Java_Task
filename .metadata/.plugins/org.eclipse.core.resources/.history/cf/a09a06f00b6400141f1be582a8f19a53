package loop;

public class Loop {
	public static String even()
	{
		String result="";
		int sum=0;
		int quantity=0;
		for(int i=1;i<=99;i++)
		{
			if(i%2==0)
			{
				sum+=i;
				quantity+=1;
			};
		}
		return result="Summ = " + sum + " Quantity = " + quantity;
	}
	public static boolean num(int a)
	{
		if(a/a==1 & a/1!=0 && (a%2!=0 || a==2) && (a%3!=0 || a==3)) b=true;
		else b=false;
		return b;
	}
	
	

}
