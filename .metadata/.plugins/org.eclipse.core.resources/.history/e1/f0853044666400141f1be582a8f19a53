	public void testFuctionWeekThuersday() 
	{
		String res = Function.week(1); 
		System.out.println(res);
		assertEquals("Tuesday", res);
	}
	@Test
	public void testFuctionWeek() 
	{
		String res = Function.week(2); 
		System.out.println(res);
		assertEquals("Monday", res);
	}
	@Test
