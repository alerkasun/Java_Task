package func;

import static org.junit.Assert.*;

import org.junit.Test;

public class FunctionTest {

	@Test
	public void testFuctionWeekMonday() 
	{
		String res = Function.week(0); 
		System.out.println(res);
		assertEquals("Monday", res);
	}
